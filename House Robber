class Solution {
public:
    int solve(vector<int>& nums,int n)
    {
        if(n<0){
          return 0;
        }
        int incl=nums[n]+solve(nums,n-2);
        int excl=solve(nums,n-1);
        return max(incl,excl);
    }
    int rob(vector<int>& nums) {
        int n=nums.size();
        return solve(nums,n-1);
    }
};
